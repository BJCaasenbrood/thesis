file = {:home/brandon/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Renda et al. - 2018 - Discrete Cosserat Approach for Multisection Soft Manipulator Dynamics.pdf:pdf},
issn = {15523098},
