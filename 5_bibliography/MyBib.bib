issn = {0018-9286},
journal = {IEEE Transactions on Automatic Control},
keywords = {Adaptive control,Computer aided software engineering,Control systems,Convergence,Manipulator dynamics,PD control,Programmable control,Robot sensing systems,Robust control,Service robots,Uncertain systems,adaptive control,globally convergent adaptive controller,high-speed two-degree-of-freedom semi-direct-drive,manipulator dynamics,parameter estimation,position control,robots,trajectory control,two-term control},
