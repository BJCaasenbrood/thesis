file = {:home/brandon/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Marchese, Rus - 2016 - Design, kinematics, and control of a soft spatial fluidic elastomer manipulator.pdf:pdf},
